-->
<config xmlns="http://moller.javapeg.config.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://moller.javapeg.config.com config.xsd">
  <logging>
    <developerMode>false</developerMode>
    <fileName>javapeg.log</fileName>
    <level>INFO</level>
    <rotate>true</rotate>
    <rotateSize>1048576</rotateSize>
    <rotateZip>true</rotateZip>
    <timestampFormat>yyyy-MM-dd'T'HH:mm:ss:SSSZ</timestampFormat>
  </logging>
  <javapegClientId>NOT-DEFINED</javapegClientId>
  <gui>
    <selectedMainGUITab>MERGE</selectedMainGUITab>
    <main>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>1280</width>
      <height>768</height>
      <splitPane id="main" width="THIN">150</splitPane>
      <splitPane id="vertical" width="THIN">300</splitPane>
      <splitPane id="thumbNailMetaDataPanel" width="THIN">640</splitPane>
      <splitPane id="previewAndComment" width="THIN">160</splitPane>
      <splitPane id="previewCommentCategoriesRating" width="THIN">400</splitPane>
      <splitPane id="mainToImageList" width="THIN">800</splitPane>
      <splitPane id="imageMergeDirectoriesToProcessLog" width="THIN">200</splitPane>
      <splitPane id="imageSearchImageExifMetaDataToRatingCommentAndButton" width="THIN">200</splitPane>
      <splitPane id="imageSearchCategoriesToImageExifMetaDataAndRatingCommentAndButton" width="THIN">200</splitPane>
    </main>
    <imageViewer>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>1024</width>
      <height>768</height>
      <splitPane id="imageMetaData" width="THIN">500</splitPane>
    </imageViewer>
    <imageSearchResultViewer>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>1024</width>
      <height>768</height>
    </imageSearchResultViewer>
    <configViewer>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>800</width>
      <height>600</height>
    </configViewer>
    <helpViewer>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>800</width>
      <height>600</height>
    </helpViewer>
    <imageResizer>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>1280</width>
      <height>700</height>
      <splitPane id="vertical" width="THIN">440</splitPane>
    </imageResizer>
    <imageConflictViewer>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>300</width>
      <height>600</height>
    </imageConflictViewer>
    <imageRepositoryStatisticsViewer>
      <xLocation>10</xLocation>
      <yLocation>10</yLocation>
      <width>800</width>
      <height>600</height>
    </imageRepositoryStatisticsViewer>
  </gui>
  <guiTabs>
    <tab id="applicationMode">
      <textColor>000000</textColor>
      <position>TOP</position>
    </tab>
  </guiTabs>
  <updatesChecker>
    <enabled>true</enabled>
    <attachVersionInformation>true</attachVersionInformation>
    <timeout>60</timeout>
    <urlVersion>http://javapeg.sourceforge.net/PAGES/updates/version.php</urlVersion>
    <urlVersionInformation>http://javapeg.sourceforge.net/PAGES/updates/versionInformation.php</urlVersionInformation>
  </updatesChecker>
  <language>
    <automaticSelection>false</automaticSelection>
    <gUILanguageISO6391>en</gUILanguageISO6391>
  </language>
  <renameImages>
    <pathSource/>
    <pathDestination/>
    <templateSubDirectoryName>Images %d%</templateSubDirectoryName>
    <templatesSubDirectoryName>
      <template>Images %d%</template>
    </templatesSubDirectoryName>
    <templateFileName>Image %d% Time %t%</templateFileName>
    <templatesFileName>
      <template>Image %d% Time %t%</template>
    </templatesFileName>
    <createThumbNails>true</createThumbNails>
    <useLastModifiedDate>true</useLastModifiedDate>
    <useLastModifiedTime>true</useLastModifiedTime>
    <cameraModelNameMaximumLength>0</cameraModelNameMaximumLength>
    <progressLogTimestampFormat>yyyy-MM-dd' 'HH:mm:ss:SSS</progressLogTimestampFormat>
  </renameImages>
  <resizeImages>
    <height>100</height>
    <width>150</width>
    <pathDestination/>
    <selectedQualityIndex>0</selectedQualityIndex>
  </resizeImages>
  <imageViewerState>
    <automaticallyRotateImages>false</automaticallyRotateImages>
    <automaticallyResizeImages>false</automaticallyResizeImages>
    <showNavigationImage>true</showNavigationImage>
    <resizeQuality>QUALITY</resizeQuality>
    <slideShowDelayInSeconds>3</slideShowDelayInSeconds>
  </imageViewerState>
  <imageSearchResultViewerState>
    <numberOfImagesToDisplay>200</numberOfImagesToDisplay>
  </imageSearchResultViewerState>
  <tagImages>
    <categories>
      <warnWhenRemove>true</warnWhenRemove>
      <warnWhenRemoveWithSubCategories>true</warnWhenRemoveWithSubCategories>
      <orRadioButtonIsSelected>true</orRadioButtonIsSelected>
    </categories>
    <paths>
      <addToRepositoryPolicy>2</addToRepositoryPolicy>
    </paths>
    <preview>
      <useEmbeddedThumbnail>true</useEmbeddedThumbnail>
    </preview>
  </tagImages>
  <thumbNail>
    <cache>
      <enabled>true</enabled>
      <maxSize>512</maxSize>
    </cache>
    <creation>
      <ifMissingOrCorrupt>true</ifMissingOrCorrupt>
      <algorithm>SMOOTH</algorithm>
      <width>160</width>
      <height>120</height>
    </creation>
    <grayfilter>
      <pixelsBrightened>true</pixelsBrightened>
      <percentage>35</percentage>
    </grayfilter>
  </thumbNail>
  <toolTips>
    <overviewState>1</overviewState>
    <imageSearchResultState>1</imageSearchResultState>
    <overviewImageViewerState>1</overviewImageViewerState>
  </toolTips>
  <categories highest-used-id="0">
  </categories>
  <importedcategories>
  </importedcategories>
  <repository>
    <paths>
    </paths>
    <exceptions>
      <allwaysAdd/>
      <neverAdd/>
    </exceptions>
  </repository>
  <metadata>
    <isoFilters>
    </isoFilters>
    <exposureTimeFilters>
    </exposureTimeFilters>
  </metadata>
</config>